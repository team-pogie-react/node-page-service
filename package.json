{
  "name": "nexus-node-composite",
  "version": "1.1.10",
  "description": "",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "dev": "npx nodemon --require @babel/register src/server.js",
    "serve": "node ./dist/server.js",
    "watch": "npx babel ./src -d ./dist -w --copy-files",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "build": "rimraf ./dist && npx babel ./src -d ./dist --copy-files",
    "test": "npx mocha --opts ./tests/mocha.opts --exit",
    "test:all": "npm run test -- --recursive ./tests/",
    "test:unit": "npm run test -- --recursive ./tests/unit",
    "test:integration": "npm run test -- --recursive ./tests/integration",
    "test:ci": "npx mocha --opts ./tests/mocha.opts --exit --reporter mocha-junit-reporter --reporter-options ./test-results.xml",
    "release": "np"
  },
  "pre-push": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.usautoparts.com/usap/nexus-node-composite"
  },
  "author": "US Auto Parts",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "chai-subset": "^1.6.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.3",
    "mocha-junit-reporter": "^1.23.1",
    "nodemon": "^1.18.11",
    "np": "^5.0.3",
    "npx": "^10.2.0",
    "pre-push": "^0.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "axios": "^0.19.0",
    "bluebird": "^3.5.4",
    "body-parser": "^1.18.3",
    "cache-manager": "^2.9.1",
    "cache-manager-redis-store": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.2",
    "is-url": "^1.2.4",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "newrelic": "^5.9.1",
    "number-to-words": "^1.2.4",
    "pidusage": "^2.0.17",
    "qs": "^6.7.0",
    "request-ip": "^2.1.3",
    "uuid": "^3.3.3",
    "validatorjs": "^3.15.1",
    "winston": "^3.2.1"
  }
}
