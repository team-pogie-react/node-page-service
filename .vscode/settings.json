{
  "files.insertFinalNewline": false,
  "editor.tabCompletion": "on",
  "editor.rulers": [
    100
  ],
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 100,
  "editor.insertSpaces": true,
  "eslint.enable": true,
  // beautify
  "beautify.config": {
    "indent_size": 2,
    "css": {
      "indent_size": 2
    },
    "end_with_newline": true,
    "brace_style": "collapse,preserve-inline",
    "operator_position": "preserve-newline"
  }
}
